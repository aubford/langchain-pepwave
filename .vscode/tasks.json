{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Open in Jetbrains Python",
      "command": "dataspell",
      "args": ["--line", "${lineNumber}", "${file}"],
      "presentation": {
        "reveal": "silent",
        "showReuseMessage": false,
        "close": true,
        "focus": false,
        "revealProblems": "never",
        "echo": false
      }
    }
  ]
}
