{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Open in Jetbrains Python",
      "command": "cursor-open-dataspell",
      "args": ["--line", "${lineNumber}", "${file}"],
      "runOptions": {
        "instanceLimit": 1
      },
      "isBackground": true,
      "type": "shell",
      "presentation": {
        "reveal": "silent",
        "showReuseMessage": false,
        "close": true,
        "focus": false,
        "revealProblems": "never",
        "echo": false
      }
    }
  ]
}
